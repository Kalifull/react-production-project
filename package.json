{
  "name": "react-production-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=4000",
    "start:dev:server": "node ./json-server/index.js",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint 'src/**/*.scss' --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "loki test",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" \"deelay\"",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.ts",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "analyze:dev": "npm run build:dev --env analyze=true",
    "analyze:prod": "npm run build:prod --env analyze=true",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "Alexeu Vorona",
  "license": "ISC",
